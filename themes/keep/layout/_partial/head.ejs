<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="keywords" content="<%= page.keywords || config.keywords   %>">
    <meta name="description" content="<%= page.description || config.description   %>">
    <meta name="author" content="<%= theme.base_info.author || config.author   %>">

    <%
    let title = page.title;
    if (is_archive()) {
        title = __('archive');
        if (is_month()) {
            title += ': ' + page.year + '/' + page.month;
        } else if (is_year()) {
            title += ': ' + page.year;
        }
    } else if (is_category()) {
        title = __('category') + ': ' + page.category;
    } else if (is_tag()) {
        title = __('tag') + ': ' + page.tag;
    } else {
        title = __(page.title);
    }
    %>
    <title>
        <% if (title) { %>
            <%= title %> |
        <% } %>
        <%= theme.base_info.title || config.title   %>
    </title>
    <%- css('css/style') %>
    <%- favicon_tag(theme.style.favicon) %>
    <%- __css('css/font-awesome.min.css') %>  
       
    <%- __css('js/prismjs/plugins/line-numbers/prism-line-numbers.min.css') %>  
    <%- __css('js/prismjs/plugins/toolbar/prism-toolbar.min.css') %>    
    

    <%- export_config() %>
</head>

